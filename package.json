  },
  "homepage": "https://github.com/ericelliott/cuid2#readme",
  "devDependencies": {
    "eslint": "8.30.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "next": "13.1.1",
    "pre-commit": "1.2.2",
    "prettier": "2.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "release-it": "15.5.1",
    "riteway": "7.0.0",
    "tap-nirvana": "1.1.0",
    "updtr": "4.0.0",
    "watch": "1.0.2"
  },
  "dependencies": {
    "@noble/hashes": "^1.1.5"
  },
